<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport"
        content="width=device-width,initial-scale=0.45 minimum-scale=0, maximum-scale=0.45, user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="shortcut icon" href=<%=baseUrl+"/images/"+faviconIcon %>>
    <link rel="stylesheet" href=<%=baseUrl+"/css/base.css"%>>
    <link rel="stylesheet" href=<%=baseUrl+"/css/swtg03.css"%>>
</head>

<body class="btnText">
    <div class="content">
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_01.jpg" %> alt="">
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_02.jpg" %> alt="">
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_03.jpg" %> alt="">
        <div class="bg_img">
            <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_04.jpg" %> alt="">
            <div class="bg_btn">
                <div class="btn "></div>
                <div class="btn "></div>
            </div>
        </div>
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_05.jpg" %> alt="">
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_06.jpg" %> alt="">
        <img class="bg_img" src=<%=baseUrl+"/images/swtg03/bg1_07.jpg" %> alt="">
    </div>
    <!-- <script src="/simJQ.js"></script> -->
    <script src=<%=cdnUrl +  "/simJQ.js" %>></script>
    <%if(isIOS){%> <%#ios%> <%if(iosIntro){%> <%#引导%> <%include
    ./components/guide%> <%}%> <%if(!iosUrl.isAppStore){%> <%#企业包%> <%include
    ./components/qyb%> <%} else{%> <%#商店包%> <%include ./components/store%>
    <%}%>
    <!-- <script src="/clipboard.min.js"></script> -->
    <script src=<%= cdnUrl + "/clipboard.min.js" %>> </script>
    <script>
        new ClipboardJS('.btnText', { text: function () { return "<%-channel%>".trim() } });
        var weixin = <%=isWx%>;
        var baseUrl = "<%-baseUrl%>";
        // $(".qrcode").css("display", "none");
        if (weixin) {
            //ios微信
            $("body").append(`<div class="fullImg"><img src="${baseUrl}/images/live_weixin_ios.png"></div>`);
        }
    </script>
    <%}else if(isAdr) {%> <%#安卓%> <%include ./components/android%>
    <script>
        var weixin = <%=isWx%>;
        var baseUrl = "<%-baseUrl%>";
        //  $(".qrcode").css("display", "none");
        if (weixin) {
            //安卓微信
            $("body").append(`<div class="fullImg"><img src="${baseUrl}/images/live_weixin.png"></div>`)
        }
    </script>
    <%} else {%> <%#其他（pc）%> <%include ./components/android%> <%}%>
    <script>
        <% include index.js %>
            document.registerReady(function () {
                $(".btnDownload2,.btn,.game").click(function () {
                    downloadFuc();
                })
            })
    </script>
    <% include ../../../common/stats/index.ejs %>
</body>

</html>